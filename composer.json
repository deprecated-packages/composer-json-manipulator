{
    "name": "symplify/composer-json-manipulator",
    "description": "Package to load, merge and save composer.json file(s)",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symfony/config": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/filesystem": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symplify/package-builder": "^9.4.4",
        "symplify/smart-file-system": "^9.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ComposerJsonManipulator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ComposerJsonManipulator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
